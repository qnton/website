{
  "private": true,
  "workspaces": [
    "web"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx,json,mdx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,js,jsx,json,mdx}\""
  },
  "devDependencies": {
    "turbo": "^1.7.3"
  },
  "optionalDependencies": {
    "turbo-android-arm64": "1.4.7",
    "turbo-darwin-64": "1.8.1",
    "turbo-darwin-arm64": "1.7.4",
    "turbo-freebsd-64": "1.4.7",
    "turbo-freebsd-arm64": "1.4.7",
    "turbo-linux-32": "1.4.7",
    "turbo-linux-64": "1.7.3",
    "turbo-linux-arm": "1.4.7",
    "turbo-linux-arm64": "1.8.1",
    "turbo-linux-mips64le": "1.4.7",
    "turbo-linux-ppc64le": "1.4.7",
    "turbo-windows-32": "1.4.7",
    "turbo-windows-64": "1.8.1",
    "turbo-windows-arm64": "1.7.4"
  }
}
